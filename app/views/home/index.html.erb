<h1>Posts</h1>

<table>
  <thead>
    <tr>
      <th><%= t(".title") %></th>
      <th><%= t(".category") %></th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= post.title %></td>
        <td><%= post.category.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>

